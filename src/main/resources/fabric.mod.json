{
	"schemaVersion": 1,
	"id": "acolectives",
	"version": "${version}",
	"name": "Accumulative Collections",
	"description": "This mod adds too much stuff to list! Not meant to be a direct upgrade of Minecraft.",
	"authors": [
		"BovineCreatesStuff!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/acolectives/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.bovine.acolectives.AccumulativeCollections"
		],
		"client": [
			"net.bovine.acolectives.AccumulativeCollectionsClient"
		],
		"fabric-datagen": [
			"net.bovine.acolectives.AccumulativeCollectionsDataGenerator"
		]
	},
	"mixins": [
		"acolectives.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}